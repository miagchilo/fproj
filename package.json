{
  "name": "fp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.26.1",
    "eslint-plugin-prettier": "^4.0.0",
    "msw": "^0.39.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "storybook": "^6.4.19",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "lint": "eslint src",
    "build-storybook": "build-storybook -s public",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@storybook/addon-actions": "^6.5.0-alpha.46",
    "@storybook/addon-essentials": "^6.5.0-alpha.46",
    "@storybook/addon-interactions": "^6.5.0-alpha.46",
    "@storybook/addon-links": "^6.5.0-alpha.46",
    "@storybook/builder-webpack5": "^6.5.0-alpha.46",
    "@storybook/manager-webpack5": "^6.5.0-alpha.46",
    "@storybook/node-logger": "^6.5.0-alpha.46",
    "@storybook/preset-create-react-app": "^4.0.1",
    "@storybook/react": "^6.5.0-alpha.46",
    "@storybook/testing-library": "0.0.9",
    "babel-plugin-named-exports-order": "0.0.2",
    "eslint": "^8.10.0",
    "eslint-plugin-react": "^7.29.3",
    "husky": "^7.0.0",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "prop-types": "^15.8.1",
    "webpack": "^5.70.0"
  }
}
